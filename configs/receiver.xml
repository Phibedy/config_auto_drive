<framework>
    <execution>
        <mainThread />
    </execution>
    <modulesToEnable>
        <module logLevel="INFO">socket_data_receiver</module>
        <module logLevel="ERROR">image_converter_scaleup</module>
        <module logLevel="ERROR">socket_datatype_definer</module>
        <!--module logLevel="DEBUG">car_to_matlab</module-->
    </modulesToEnable>
    <service>
        <name>warp_service</name>
        <!-- camera/cali.lconf - new car -->
        <!-- camera/cali_cc15_auto.lconf - old car -->
        <!--config src="camera/homography_bmw.lconf"/-->
        <config src="camera/cali_cc2016_hansi.lconf" />
        <config src="camera/homography_cc2016_hansi.lconf" />
    </service>
    <module>
        <name>socket_datatype_definer</name>
    </module>
    <module>
        <name>socket_data_receiver</name>
        <config>
            <dataChannels>IMAGE_GREY_SCALE,CAR,TRAJECTORY_LINE,TRAJECTORY_POINT,ENVIRONMENT_OBSTACLE,MIDDLE_LANE</dataChannels>
            <ip>192.168.0.112</ip>
            <!--ip>localhost</ip-->
            <port>65001</port>
        </config>
    </module>
    <module>
        <name>car_to_matlab</name>
    </module>
    <module>
        <name>image_converter_scaleup</name>
        <realName>image_converter</realName>
        <channelMapping from="INPUT_IMAGE" to="IMAGE_GREY_SCALE"/>
        <channelMapping from="OUTPUT_IMAGE" to="GREY_IMAGE"/>
        <config>
            <scaleUp>4</scaleUp>
        </config>
    </module>
    <include src="render.xml"/>
</framework>
