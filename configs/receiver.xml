<framework>
    <execution>
        <clock enabled="true" unit="hz" value="100" />
    </execution>
    <modulesToEnable>
        <module logLevel="DEBUG">socket_data_receiver</module>
        <module logLevel="DEBUG">image_converter_scaleup</module>
        <module logLevel="DEBUG">car_to_matlab</module>
    </modulesToEnable>
    <module>
        <name>socket_data_receiver</name>
        <libpath>socket_data/receiver</libpath>
        <config src="socket_data/pc_socket_data_receiver.lconf" />
    </module>
    <module>
        <name>car_to_matlab</name>
    </module>
    <module>
        <name>image_converter_scaleup</name>
        <realName>image_converter</realName>
        <channelMapping from="INPUT_IMAGE" to="IMAGE_GREY_SCALE"/>
        <channelMapping from="OUTPUT_IMAGE" to="GREY_IMAGE"/>
        <config>
            <scaleUp>5</scaleUp>
        </config>
    </module>
    <include src="render.xml"/>
</framework>
