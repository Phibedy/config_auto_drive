
        <!-- ./lms -c mycam --flags files --enable-load /home/phibedy/CC2016/neue_daten/lmslog/2016-01-25T17-01-22Z-normale_runde-->
<framework>
        <service>
            <name>warp_service</name>
            <!-- camera/cali.lconf - new car -->
            <!-- camera/cali_cc15_auto.lconf - old car -->
            <!--config src="camera/cali_cc2016_laeuft_bei_uns.lconf" /-->
            <config src="camera/homography_ximea.lconf" />
        </service>
        <service>
            <name>AreaOfDetection</name>
            <realName>area_of_detection</realName>
            <config src="visible_areas.lconf"/>
        </service>
        <include src="image.xml"/>
        <!-- Laserscanner -->
        <modulesToEnable logLevel="WARN">
            <module>laser_scanner</module>
        </modulesToEnable>
        <module>
            <name>laser_scanner</name>
            <config>
                <x>0.09</x>
                <y>0.05</y>
                <channelMapping from="URG_DATA" to="URG_DATA"/>
                <channelMapping from="URG_DATA_RAW" to="URG_DATA_RAW"/>
            </config>
        </module>
</framework>
