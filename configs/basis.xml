<framework>
	<modulesToEnable>
		<module>image_hint_worker</module>
		<module logLevel="DEBUG">image_hint_transformer</module>
		<module logLevel="WARN">environment_predictor</module>
		<module>trajectory_line_creator</module>
		<module>trajectory_point_creator</module>
		<module logLevel="WARN">image_hint_generator</module>
		<module logLevel="WARN">environment_filter</module>
		<module logLevel="WARN">trajectory_point_controller</module>
		<module logLevel="WARN">velocity_controller</module>
		<module logLevel="WARN">sensor_tracker</module>
        <module>image_proxy</module>
	</modulesToEnable>
	<module>
	    <name>environment_logger</name>
		<channelMapping from="ENVIRONMENT_INPUT" to="ENVIRONMENT_ROAD"/>
		<config>
		    <directory>/home/phibedy/Dokumente/programmieren/lms_repos/config_auto_drive/logging_data</directory>
		</config>
	</module>
	<module>
		<name>velocity_controller</name>
		<channelMapping from="ENVIRONMENT_INPUT" to="ENVIRONMENT_ROAD_MERGED"/>
		<config>
		    <maxSpeed>1.5</maxSpeed>
		    <minCurveSpeed>0.5</minCurveSpeed>
		    <forcastLength>0.5</forcastLength>
		    <maxDeltaTInMs>15</maxDeltaTInMs>
		    <maxAcc>4</maxAcc>
		    <minAcc>-6</minAcc>
		</config>
    </module>
	<module>
		<name>trajectory_point_controller</name>
		<channelMapping from="POINT" to="TRAJECTORY_POINT"/>
		<config>
		    <maxSpeed>1</maxSpeed>
		    <aggro>0.95</aggro>
		</config>
    </module>
	<module>
	    <name>image_hint_generator</name>
		<writePriority>10</writePriority>
		<channelMapping from="HINTS" to="HINTS"/>
		<channelMapping from="TARGET_IMAGE" to="GREY_IMAGE"/>
		<channelMapping from="ENV_MID" to="ENVIRONMENT_ROAD_MERGED"/>
		<config name="defaultLPParameter" src="imaging/defaultEdgePointParam.lconf"/>
	    <config name="defaultLPParameter" src="imaging/defaultLinePointParam.lconf"/>
	    <config name="defaultLPParameter" src="imaging/defaultLineParam.lconf"/>
	</module>
	<module>
	    <name>sensor_tracker</name>
	</module>
	<module>
	    <name>trajectory_point_creator</name>
		<channelMapping from="LINE" to="TRAJECTORY_LINE"/>
		<channelMapping from="POINT" to="TRAJECTORY_POINT"/>
		<config>
			<distanceSearched> 0.4 </distanceSearched>
		</config>
	</module>
	<module>
	    <name>trajectory_line_creator</name>
		<channelMapping from="ENVIRONMENT" to="ENVIRONMENT_ROAD_MERGED"/>
		<channelMapping from="LINE" to="TRAJECTORY_LINE"/>
		<config src="environment.lconf" />
	</module>
	<module>
	    <name>environment_filter</name>
		<channelMapping from="ENVIRONMENT_INPUT" to="ENVIRONMENT_ROAD_RAW"/>
		<channelMapping from="ENVIRONMENT_OUTPUT" to="ENVIRONMENT_ROAD_FILTERED"/>
	</module>
	<module>
	    <name>environment_predictor</name>
	    <config>
	        <elementCount>10</elementCount>
	        <r_fakt>1000</r_fakt>
	    </config>
		<channelMapping from="ENVIRONMENT_INPUT" to="ENVIRONMENT_ROAD_FILTERED"/>
		<channelMapping from="ENVIRONMENT_OUTPUT" to="ENVIRONMENT_ROAD_MERGED"/>
	</module>
	<module>
		<name>image_hint_transformer</name>
		<channelMapping from="ENVIRONMENT" to="ENVIRONMENT_ROAD_RAW"/>
	</module>
	<module>
	    <name>image_hint_worker</name>
	</module>
	<module>
		<name>image_converter_trans</name>
		<realName>image_converter</realName>	
	    <channelMapping from="INPUT_IMAGE" to="GREY_IMAGE"/>
	    <channelMapping from="OUTPUT_IMAGE" to="TRANS_IMAGE"/>
	    <config>
		    <output_format>GREY</output_format>
		    <filter>imageV2C</filter>
	    </config>
	</module>
    <module>
        <name>image_proxy</name>
        <config>
            <displayMode>DIRECTORY</displayMode>
        </config>
        <config user="phibedy">
            <directory>/home/phibedy/Dokumente/programmieren/c++/lms_repos/config_auto_drive/test_images/20150519-164907</directory>
            <singleFile>/home/phibedy/Dokumente/programmieren/c++/lms_repos/config_auto_drive/test_images/20150519-164907/0000.pgm</singleFile>
        </config>
        <config user="hans">
            <directory>/home/hans/workspace/config_auto_drive/test_images/20150519-164907</directory>
            <singleFile>/home/hans/workspace/config_auto_drive/test_images/20150519-164907/0050.pgm</singleFile>
        </config>
        <config user="tobi">
            <directory>/home/tobi/phoenix_code/repos/config_auto_drive/test_images/20150519-164907</directory>
            <singleFile>/home/tobi/phoenix_code/repos/config_auto_drive/test_images/20150512-181426/0050.pgm</singleFile>
        </config>
        <channelMapping from="INPUT_IMAGE" to="GREY_IMAGE_PRE" />
        <channelMapping from="OUTPUT_IMAGE" to="GREY_IMAGE"/>
    </module>
</framework>
